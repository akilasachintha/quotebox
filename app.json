{
  "expo": {
    "name": "QuoteBox",
    "description": "A simple app to store your favorite quotes",
    "slug": "quote-box",
    "owner": "asynclabs",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "scheme": "com.quotebox",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#ffffff",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/assets/splash-with-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.quotebox",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "30b9a2c3-a4fd-43f2-919a-9ba7c020c429"
      }
    }
  }
}
